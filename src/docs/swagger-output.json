    },
    "version": "1.0.0"
  },
  "host": "localhost:3000",
  "basePath": "/",
  "schemes": "['http','https']",
  "securityDefinitions": {
    "bearerAuth": {
      "type": "apiKey",
      "in": "header",
      "name": "Authorization",
      "description": "Bearer Token",
      "scheme": "bearer",
      "bearerFormat": "JWT"
    }
  },
  "consumes": [
    "application/json"
  ],
  "produces": [
    "application/json"
  ],
  "paths": {
    "/getAllUser": {
      "get": {
        "tags": [
          "Profile"
        ],
        "summary": "Get all users",
        "description": "",
        "parameters": [
          {
            "name": "pageNumber",
            "in": "query",
            "type": "string"
          },
          {
            "name": "pageSize",
            "in": "query",
            "type": "string"
          },
          {
            "name": "name",
            "in": "query",
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/getById/{userId}": {
      "get": {
        "tags": [
          "Profile"
        ],
        "summary": "Get by Id",
        "description": "",
        "parameters": [
          {
            "name": "userId",
            "in": "path",
            "required": true,
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/userUpdate/{userId}": {
      "put": {
        "tags": [
          "Profile"
        ],
        "summary": "profile update API",
        "description": "",
        "parameters": [
          {
            "name": "userId",
            "in": "path",
            "required": true,
            "type": "string"
          },
          {
            "name": "body",
            "in": "body",
            "description": "user can update their profile here",
            "required": true,
            "schema": {
              "type": "object",
              "properties": {
                "name": {
                  "type": "string",
                  "example": "Abul Faiz"
                },
                "email": {
                  "type": "string",
                  "example": "abulfaiz428@gmail.com"
                },
                "password": {
                  "type": "string",
                  "example": "Login@123"
                },
                "role": {
                  "type": "string",
                  "example": "ADMIN"
                },
                "favoriteGenres": {
                  "type": "array",
                  "example": [
                    "Action",
                    "Sci-Fi"
                  ],
                  "items": {
                    "type": "string"
                  }
                }
              }
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          },
          "400": {
            "description": "Bad Request"
          }
        }
      }
    },
    "/deleteUser/{userId}": {
      "delete": {
        "tags": [
          "Profile"
        ],
        "summary": "Delete user API",
        "description": "",
        "parameters": [
          {
            "name": "userId",
            "in": "path",
            "required": true,
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    }
  },
  "security": [
    {
      "bearerAuth": []
    }
  ]
}