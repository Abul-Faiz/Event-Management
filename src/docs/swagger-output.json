      "post": {
        "tags": [
          "Authentication"
        ],
        "summary": "SignUp API",
        "description": "",
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "description": "user or admin can signUp here",
            "required": true,
            "schema": {
              "type": "object",
              "properties": {
                "name": {
                  "type": "string",
                  "example": "Abul Faiz"
                },
                "email": {
                  "type": "string",
                  "example": "abulfaiz428@gmail.com"
                },
                "password": {
                  "type": "string",
                  "example": "Login@123"
                },
                "role": {
                  "type": "string",
                  "example": "ADMIN"
                }
              }
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          },
          "400": {
            "description": "Bad Request"
          }
        }
      }
    },
    "/login": {
      "post": {
        "tags": [
          "Authentication"
        ],
        "summary": "Login API",
        "description": "",
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "description": "User or Admin can login here",
            "required": true,
            "schema": {
              "type": "object",
              "properties": {
                "email": {
                  "type": "string",
                  "example": "jay@gmail.com"
                },
                "password": {
                  "type": "string",
                  "example": "Login@123"
                }
              }
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          },
          "400": {
            "description": "Bad Request"
          }
        }
      }
    },
    "/create-Event": {
      "post": {
        "tags": [
          "Event"
        ],
        "summary": "create events",
        "description": "",
        "parameters": [
          {
            "name": "Authorization",
            "in": "header",
            "type": "string"
          },
          {
            "name": "body",
            "in": "body",
            "description": "Event object",
            "schema": {
              "type": "object",
              "properties": {
                "eventName": {
                  "type": "string",
                  "example": "Event Management"
                },
                "description": {
                  "type": "string",
                  "example": "A small mini project with proper base setup"
                }
              }
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          },
          "400": {
            "description": "Bad Request"
          },
          "403": {
            "description": "Forbidden"
          },
          "404": {
            "description": "Not Found"
          }
        }
      }
    },
    "/join-Event": {
      "post": {
        "tags": [
          "Event"
        ],
        "summary": "Join event",
        "description": "",
        "parameters": [
          {
            "name": "Authorization",
            "in": "header",
            "type": "string"
          },
          {
            "name": "eventId",
            "in": "query",
            "type": "string"
          },
          {
            "name": "userId",
            "in": "query",
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          },
          "403": {
            "description": "Forbidden"
          },
          "404": {
            "description": "Not Found"
          }
        }
      }
    },
